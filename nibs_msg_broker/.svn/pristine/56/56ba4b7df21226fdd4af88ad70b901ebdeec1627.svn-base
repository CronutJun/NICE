		            FROM 	OP.T_IF_MISSNO_CHK 
		            WHERE 	trans_seq_no <= (select MAX(trans_seq_no)+1 from OP.t_fn_realtime_TRADE_KIOSK where org_cd = '0HP' AND DEAL_DATE = TO_CHAR(SYSDATE-1, 'YYYYMMDD'))
