                AND     REQ_TYPE = 'Q'
                AND     EXT_COM_CD = '096'
                AND     FORMAT_TYPE = 'CM'
                MINUS
                SELECT     TRANS_DATE as gw_date, lpad(TO_CHAR(TRANS_SEQ_NO), 7, '0')
                FROM     op.T_IF_DATA_LOG
                WHERE     ORG_CD = '0EM'
                AND     TRANS_DATE between TO_CHAR(SYSDATE - 2, 'YYYYMMDD') and to_char(sysdate, 'yyyymmdd')
