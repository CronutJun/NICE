<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ADD_CASH">

<!--
<entry key="ADD_CASH"               value="03001160" />     06. 추가현송

{
   "name" : "03001160",                    "kname" : "추가현송 보고서",
   "response" : { "code" : "03001160",     "type" : "SELF" },
   "columns" : [
      { "name" : "CM",                     "kname" : "공통헤더",               "type" : "INC", "include" : "comm_msg_header.json" },
      { "name" : "brch_cd",                "kname" : "관리점코드",             "type" : "AN",  "length"  : 8   },
      { "name" : "mac_no",                 "kname" : "기번",                   "type" : "AN",  "length"  : 12  },
      { "name" : "org_site_cd",            "kname" : "기관코너번호",           "type" : "AN",  "length"  : 6   },
      { "name" : "site_type",              "kname" : "점포구분",               "type" : "AN",  "length"  : 4   },
      { "name" : "oper_type",              "kname" : "운영구분",               "type" : "AN",  "length"  : 4   },
      { "name" : "org_site_nm",            "kname" : "장소명",                 "type" : "AN",  "length"  : 60  },
      { "name" : "safe_type",              "kname" : "금고구분 1:코너, 2:센터 이외:기기",  "type" : "AN",  "length"  : 4   },
      { "name" : "addcash_type",           "kname" : "추가현송구분 B-익일배치, R-실시간",  "type" : "AN",  "length"  : 1   },
      { "name" : "pda_yn",                 "kname" : "PDA 여부",               "type" : "AN",  "length"  : 1   },
      { "name" : "serial_no",              "kname" : "추가현송 시리얼번호",    "type" : "AN",  "length"  : 3   },
      { "name" : "add_date",               "kname" : "추가현송일자",           "type" : "N",   "length"  : 8   },
      { "name" : "add_time",               "kname" : "추가현송시간",           "type" : "N",   "length"  : 6   },
      { "name" : "demand_date",            "kname" : "청구예상일자",           "type" : "N",   "length"  : 8   },
      { "name" : "add_amt",                "kname" : "추가현송액 (현금)",      "type" : "N",   "length"  : 15  },
      { "name" : "add_chk_amt",            "kname" : "추가현송수표금액",       "type" : "N",   "length"  : 15  },
      { "name" : "check_10_cnt",           "kname" : "10만원권 매수",          "type" : "N",   "length"  : 5   },
      { "name" : "check_30_cnt",           "kname" : "30만원권 매수",          "type" : "N",   "length"  : 5   },
      { "name" : "check_50_cnt",           "kname" : "50만원권 매수",          "type" : "N",   "length"  : 5   },
      { "name" : "check_100_cnt",          "kname" : "100만원권 매수",         "type" : "N",   "length"  : 5   },
      { "name" : "close_yn",               "kname" : "마감구분 0:마감전, 1:마감후",     "type" : "AN",  "length"  : 1   },
      { "name" : "send_date",              "kname" : "현송기준일자",           "type" : "N",   "length"  : 8   },
      { "name" : "office_nm",              "kname" : "관리사무소",             "type" : "AN",  "length"  : 20  },
      { "name" : "insert_time",            "kname" : "입력시간",               "type" : "N",   "length"  : 6   },
      { "name" : "cash_10000_cnt",         "kname" : "만원권 매수",            "type" : "N",   "length"  : 5   },
      { "name" : "cash_5000_cnt",          "kname" : "오천원권 매수",          "type" : "N",   "length"  : 5   },
      { "name" : "cash_1000_cnt",          "kname" : "천원권 매수",            "type" : "N",   "length"  : 5   },
      { "name" : "cash_500_cnt",           "kname" : "오백원권 매수",          "type" : "N",   "length"  : 5   },
      { "name" : "cash_100_cnt",           "kname" : "백원권 매수",            "type" : "N",   "length"  : 5   },
      { "name" : "cash_50_cnt",            "kname" : "오십원권 매수",          "type" : "N",   "length"  : 5   },
      { "name" : "cash_10_cnt",            "kname" : "십원권 매수",            "type" : "N",   "length"  : 5   },
      { "name" : "filler",                 "kname" : "여유공간",               "type" : "AN",  "length"  : 177 }
   ]
}
 -->
    <sql id="A">
        branch_cd
        ,mac_no
        ,'B' addcash_type
        ,serial_no
        ,add_date
        ,demand_date
        ,add_cash_amt add_amt
        ,close_yn
        ,send_date
        ,office_nm
    </sql>

    <sql id="B">
        OP.FC_GET_MAP_BRANCH_CD(0, ORG_CD, BRANCH_CD, MAC_NO) branch_cd
        ,OP.FC_GET_MAP_MAC_NO(0, ORG_CD, BRANCH_CD, MAC_NO) mac_no
        ,'B' addcash_type
        ,serial_no
        ,add_date
        ,demand_date
        ,add_cash_amt add_amt
        ,close_yn
        ,send_date
        ,office_nm
    </sql>

    <sql id="C">
        branch_cd
        ,mac_no
        ,'B' addcash_type
        ,'0' pda_yn
        ,serial_no
        ,add_date
        ,add_time
        ,demand_date
        ,add_cash_amt add_amt
        ,add_check_amt add_chk_amt
        ,hw15_cnt check_10_cnt
        ,hw35_cnt check_30_cnt
        ,hw55_cnt check_50_cnt
        ,hw16_cnt check_100_cnt
        ,'1' close_yn                  /* 대구은행은 무조건 마감후. 마감전이 없음. */
        ,OP.pkg_fn_common.sf_getPostActDate(add_date) send_date     /* 현송기준일 : 익영업일 */
        ,office_nm
        ,add_time insert_time
        ,add_cw14_amt/10000 cash_10000_cnt
        ,add_cw54_amt/50000 cash_50000_cnt
    </sql>

    <sql id="D">
        branch_cd
        ,mac_no
        ,safe_type
        ,'B' addcash_type
        ,'0' pda_yn
        ,serial_no
        ,add_date
        ,add_time
        ,demand_date
        ,add_cash_amt add_amt
        ,add_check_amt add_chk_amt
        ,hw15_cnt check_10_cnt
        ,hw35_cnt check_30_cnt
        ,hw55_cnt check_50_cnt
        ,hw16_cnt check_100_cnt
        ,close_yn
        ,send_date
        ,office_nm
        ,add_time insert_time
    </sql>

    <select id="default" resultType="hashmap" parameterType="com.nicetcm.nibsplus.orgsend.model.OrgSendQryParamVO">
        SELECT
            <choose>
                <when test='orgCd == "020" or orgCd == "032"'><include refid="A"></include></when>
                <when test='orgCd == "039"'><include refid="B"></include></when>
                <when test='orgCd == "031"'><include refid="C"></include></when>
                <when test='orgCd == "088"'><include refid="D"></include></when>
                <otherwise>
                    branch_cd
                    ,mac_no
                    ,'B' addcash_type
                    ,'0' pda_yn
                    ,serial_no
                    ,add_date
                    ,demand_date
                    ,add_cash_amt add_amt
                    ,close_yn
                    <choose>
                        <when test='orgCd == "035"'>
                        </when>
                        <otherwise>
                            ,add_time
                            ,add_check_amt add_chk_amt
                            ,hw15_cnt check_10_cnt
                            ,hw35_cnt check_30_cnt
                            ,hw55_cnt check_50_cnt
                            ,hw16_cnt check_100_cnt
                            ,send_date
                            ,office_nm
                            ,add_time insert_time

                            <choose>
                                <when test='orgCd == "011"'>
                                </when>
                                <otherwise>
                                    ,safe_type
                                </otherwise>
                            </choose>
                        </otherwise>
                    </choose>
                    <choose>
                       <when test='orgCd == "003"'>,add_cash_amt/10000 cash_10000_cnt</when>
                       <when test='orgCd == "011"'>
                           ,add_cw14_amt/10000 cash_10000_cnt
                           ,add_cw54_amt/50000 cash_50000_cnt
                       </when>
                    </choose>
                </otherwise>
            </choose>

        FROM OP.T_FN_ATMS_ADD_CASH_REPORT
        <where>
            AND org_send_yn = '0'
            <choose>
               <when test='orgCd == "011" or orgCd == "032" or orgCd == "031"'>and demand_date = TO_CHAR( SYSDATE, 'YYYYMMDD' )</when>
               <when test='orgCd == "020"'>AND MAC_NO IS NOT NULL</when>
               <when test='orgCd == "039"'>AND OP.FC_GET_MAP_MAC_NO(0, ORG_CD, BRANCH_CD, MAC_NO) IS NOT NULL</when>
            </choose>
            AND org_cd = #{orgCd}
        </where>
    </select>
</mapper>