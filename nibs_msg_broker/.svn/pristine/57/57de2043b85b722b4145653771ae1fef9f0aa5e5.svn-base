#USE_COL_CNT, COL_LEN, COL_TYPE 순으로 정의되어 져야 하며 SELECT 절 내부를 제외하고는 라인을 띄우면 안된다.
#SELECT 문의 COLUMN은 반드시 50개를 채워줘야 한다.(USE_COL_CNT 만큼 제외한 컬럼은 '' 등 으로 정의
USE_COL_CNT=7
COL_LEN=8,12,8,6,5,15,368
COL_TYPE=A,A,A,A,A,A,A,A,A,A,N,A,A,A,A,A,A,A,A,A,A,A
SELECT JIJUM_CD,
	 mac_no,   
	 TO_CHAR(sysdate - 1, 'YYYYMMDD'),  
	 '',  
	 '',  
	 '', 
	 '',  
	 '',  
	 '','','','','','','','','','','','','','','','','','','','','','','','','','','','','','','','','','','','','','','','','',''
FROM (
        SELECT  JIJUM_CD, MAC_NO
        FROM    T_CM_MAC
        WHERE   ORG_CD = '0TR'
        AND     OPEN_DATE <= TO_CHAR(sysdate - 1, 'YYYYMMDD')
        AND    (CLOSE_DATE IS NULL OR CLOSE_DATE >= TO_CHAR(sysdate - 1, 'YYYYMMDD'))
