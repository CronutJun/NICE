                    THEN SUBSTR(grd.BRANCH_CD, 2, 3)
                    ELSE grd.BRANCH_CD
                    END as brch_cd, */
               grd.BRANCH_CD AS brch_cd,
               grd. mac_no,
        /*     CASE WHEN grd.org_cd = '031' AND LENGTH(grd.BRANCH_CD) = 4
                    THEN CASE WHEN FC_GET_MAP_SITE_CD(1, 'N31', grd.BRANCH_CD, grd.site_cd ) IS NULL
