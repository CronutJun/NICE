#USE_COL_CNT, COL_LEN, COL_TYPE 순으로 정의되어 져야 하며 SELECT 절 내부를 제외하고는 라인을 띄우면 안된다.
#SELECT 문의 COLUMN은 반드시 50개를 채워줘야 한다.(USE_COL_CNT 만큼 제외한 컬럼은 '' 등 으로 정의
USE_COL_CNT=24
COL_LEN=8,8,6,40,100,10,10,10,10,10,10,10,10,10,10,10,10,10,10,10,10,10,10,80
COL_TYPE=N,A,A,A,A,A,A,A,A,A,A,A,A,A,A,A,A,A,A,A,A,A,A,A
SELECT     DISTINCT
          CHK.CHECK_DATE
        , F_GET_ORG_JIJUM_CD( CHK.ORG_CD, CHK.JIJUM_CD, CHK.SITE_CD, '' ) AS ORG_JIJUM_CD
        , F_GET_ORG_SITE_CD( CHK.ORG_CD, CHK.JIJUM_CD, CHK.SITE_CD, '' ) AS ORG_SITE_CD
        , SITE.SITE_NM
        , CHK.PHOTO_URL            
        , CHK.MAC_FRONT_GLASS_PHOTO
        , CHK.MAC_FRONT_PHOTO      
        , CHK.MAC_GAP_PHOTO        
        , CHK.MONITOR_PHOTO        
        , CHK.BANKBOOK_GAP_PHOTO   
        , CHK.CARD_GAP_PHOTO       
        , CHK.INTERPHONE_PHOTO     
        , CHK.FIX_PHOTO            
        , CHK.FLOOR_PHOTO          
        , CHK.IN_WALL_PHOTO        
        , CHK.OUT_WALL_PHOTO       
        , CHK.BOOTH_GLASS_PHOTO    
        , CHK.UP_PHOTO             
        , CHK.LAMP_PHOTO           
        , CHK.STICKER_PHOTO        
        , CHK.GABAGE_PHOTO         
        , CHK.OUTDOOR_HEATER_PHOTO 
        , CHK.SITE_ARROUD_PHOTO 
        , ''
        , '', '', '', '', '', '', '', '', '', '', '', '', '', '', '', '', '', '', '', '', '', '', '', '', '', ''
FROM     T_CM_SITE_ENV_CHECK    CHK,
         T_CM_SITE              SITE,
         T_CM_MEMBER            MBR,
         T_CM_OFFICE            OFC
WHERE   CHK.CHECK_DATE BETWEEN TO_CHAR( SYSDATE-30, 'YYYYMMDD' ) AND TO_CHAR( SYSDATE-1, 'YYYYMMDD' )
AND     CHK.ORG_SEND_YN   = '1'
AND		CHK.PHOTO_URL IS NOT NULL
AND		CHK.ORG_CD = '092'
AND     CHK.ORG_CD		= SITE.ORG_CD
AND     CHK.JIJUM_CD	= SITE.JIJUM_CD
AND     CHK.SITE_CD		= SITE.SITE_CD
AND     SITE.DEPT_CD    = OFC.DEPT_CD
AND     SITE.OFFICE_CD	= OFC.OFFICE_CD
AND     CHK.MEMBER_ID	= MBR.MEMBER_ID(+)

