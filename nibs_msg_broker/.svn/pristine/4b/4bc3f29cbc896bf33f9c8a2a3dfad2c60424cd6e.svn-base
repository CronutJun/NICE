#USE_COL_CNT, COL_LEN, COL_TYPE 순으로 정의되어 져야 하며 SELECT 절 내부를 제외하고는 라인을 띄우면 안된다.
#SELECT 문의 COLUMN은 반드시 50개를 채워줘야 한다.(USE_COL_CNT 만큼 제외한 컬럼은 '' 등 으로 정의
USE_COL_CNT=36
COL_LEN=8,6,40,50,10,4,4,4,4,3,1,2,15,15,15,15,15,15,15,15,15,15,2,15,15,100,6,1,1,1,1,1,10,32,8,31
COL_TYPE=A,A,A,A,A,A,A,A,A,N,N,N,A,A,A,A,A,A,A,A,A,A,N,A,A,A,A,A,A,A,A,A,A,A,A,A
SELECT   
	  JIJUM_CD       ,	/* 관리점코드					*/   	
	  SITE_CD        , 	/* 코너코드						*/   
	  JIJUM_NM       , 	/* 관리점명						*/   
	  SITE_NM        , 	/* 코너명						*/   
	  OFFICE_NM      , 	/* 사무소						*/   
	  ''         , 	
	  LOCATE_TYPE	,  	/* 점포구분=>장소구분코드						*/   
	  OPER_TIME_TYPE,   /* 운영구분=>운영시간구분코드						*/   
	  '',
	  MAC_CNT		 , 	/* 기기대수						*/   
	  FEE_TYPE		,	/* 		 용역료구분					*/
	  '', 
	  BASE_FEE       , 	/* 기본/특수용역료					*/   
	  MONTH_FEE		     , 	/* 월용역료						*/   
	  ADD_FEE        , 	/* 기기추가용역료					*/   
	  ''    , 	/* 산출용역료					*/
	  OVERTIME_FEE   , 	/* 시간초과용역료				*/   
	  CHECK_FEE      , 	/* 수표출금추가용역료				*/   
	  GUARD_MAC_FEE  , 	/* 경비장비료					*/   
	  SURTAX         , 	/* 부가세						*/   
	  DEMAND_FEE     , 	/* 확정청구용역료				*/   
	  SITE_FEE       , 	
	  OPER_DAY       , 	
	  PENALTY_AMT    , 	/* 페널티금액					*/   
	  JUM_SUM        , 	/* 지점별합계					*/   
	  BIGO           , 	/* 비고							*/   
	  YEAR_MON       , 	/* 해당연월						*/   
	  CHECK_YN       		, 	/* 수표출금운영여부				*/
	  '1'		,	  	/* 업체동의여부 1:동의,0:동의안함*/  
	  MAC_ADD_YN,				/* 기기추가구분					*/
	  OVERTIME_YN,				/* 시간추가구분					*/
	  CHECK_IN_YN,				/* 수표입금운영여부				*/
	  CHECK_IN_FEE,				/* 수표입금추가용역료			*/
	 FC_FN_SECURITY( ACC.ACCOUNT , '2') ,				/* 입금계좌						*/
	  TO_CHAR(LAST_DAY(SYSDATE),'yyyymmdd'),				/* 납입기한						*/
	  '',
	  '',
	  '','','','','','','','','','','','',''
FROM T_CT_SERVICE_FEE_MONTHLY		MON
	,T_CM_ACCOUNT					ACC
WHERE  MON.ORG_CD = '003' 
	AND MON.ORG_SEND_YN = '0'
	AND	MON.ORG_CD = ACC.ORG_CD
	AND	ACC.SERVICE_GB = '0'			/* 서비스구분	(0:일괄용역)	*/
	AND	ACC.USE_TYPE = '0'				/* 사용유형		(0:용역료계좌)	*/ 

  