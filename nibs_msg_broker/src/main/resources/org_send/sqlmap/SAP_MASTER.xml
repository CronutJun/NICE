                ,CENTER_CONFIRM
                ,UPDATE_UID
                ,REMARK BIGO
                ,''
                ,''
                ,''
                ,'','','','','','','','','','','','','','','','','','','','','','','','','','','','','','','',''
        FROM  OP.T_FN_SAP_MASTER
        WHERE DEAL_DATE >= TO_CHAR( SYSDATE -14, 'YYYYMMDD' )
        AND   office_confirm = '1'
        and   nvl(org_send_yn , '0') = '0'
