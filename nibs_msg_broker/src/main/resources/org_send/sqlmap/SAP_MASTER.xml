    <select id="selectDefault" resultType="hashmap" parameterType="com.nicetcm.nibsplus.orgsend.model.OrgSendQryParamVO">
    <![CDATA[
        SELECT   '1' info_cl /* 1:upload */
                ,DEAL_DATE
                ,BRANCH_CD
                ,''
                ,MEMBER_ID
                ,MEMBER_NM
                ,MEMBER_TYPE
                ,CHECK_AMT
                ,CASH_AMT
                ,SELF_CUPON
                ,ETC_CUPON
