#USE_COL_CNT, COL_LEN, COL_TYPE 순으로 정의되어 져야 하며 SELECT 절 내부를 제외하고는 라인을 띄우면 안된다.
#SELECT 문의 COLUMN은 반드시 50개를 채워줘야 한다.(USE_COL_CNT 만큼 제외한 컬럼은 '' 등 으로 정의
USE_COL_CNT=21
COL_LEN=8,12,6,4,4,60,8,15,15,15,15,15,15,8,15,15,2,1,20,1,168
COL_TYPE=A,A,A,A,A,A,A,A,A,A,A,A,A,A,A,A,A,A,A,A,A
SELECT JIJUM_CD,
	 MAC_NO,
	 '',
	 '',
	 '',
	 '',
     close_date,        
     before_cash_amt,   
     in_amt,            
     out_amt,           
     close_amt,         
     '',     
     '',      
     '',     
     collect_amt,       
     '',    
     '',    
     report_yn,         
     office_nm,         
     nvl(send_yn, '1'),
     '','','','','','','','','','','','','','','','','','','','','','','','','','','','','',''
FROM T_FN_ATMS_COLLECT  
WHERE CLOSE_DATE >= TO_CHAR( SYSDATE - 10, 'YYYYMMDD')
AND   org_send_yn = '0'  
AND   org_cd = '020' 
 AND MAC_NO IS NOT NULL
