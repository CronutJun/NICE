#USE_COL_CNT, COL_LEN, COL_TYPE 순으로 정의되어 져야 하며 SELECT 절 내부를 제외하고는 라인을 띄우면 안된다.
#SELECT 문의 COLUMN은 반드시 50개를 채워줘야 한다.(USE_COL_CNT 만큼 제외한 컬럼은 '' 등 으로 정의
USE_COL_CNT=15
COL_LEN=8,12,8,6,1,15,15,15,15,1,8,8,15,15,280
COL_TYPE=A,A,A,A,A,A,A,A,A,A,A,A,A,A,A
SELECT  SND.JIJUM_CD,								/* 영업점코드    					*/
        SND.MAC_NO,                                 /* 단말번호                         */
        SND.SEND_DATE,                              /* 거래일자                         */
        SND.SEND_TIME,                              /* 마감시간                         */
        CASE WHEN (SELECT COUNT(1) FROM T_FN_SEND WHERE ORG_CD = SND.ORG_CD
                                                  AND JIJUM_CD = SND.JIJUM_CD
                                                  AND MAC_NO = SND.MAC_NO
                                                  AND SEND_TYPE = '2'
                                                  AND SEND_DATE = TO_CHAR(SYSDATE,'YYYYMMDD')) > 0
                  AND 
                  (SELECT COUNT(1) FROM T_CM_CASH WHERE ORG_CD = SND.ORG_CD      /* 추가현송이고, 마감데이터가 없으면 마감전으로 셋팅 */
                                                  AND JIJUM_CD = SND.JIJUM_CD
                                                  AND MAC_NO = SND.MAC_NO
                                                  AND CASH_TYPE = '2') = 0 
              THEN '0'
            ELSE 
              '1'
            END CLOSE_YN,                                    /* 마감전(0),후(1) 구분                   */
        SND.SEND_AMT,                               /* 장전금액                         */
        '',                                     /* 현금지급액                       */
        '',                                     /* 현금입금액                       */
        '',                                     /* 현금잔액                         */
        '0',/* 0:장전, 1:장전취소 */            /* 장전취소여부						*/
     	(SELECt SITE_CD FROM T_CM_MAC 
     					WHERE ORG_CD = SND.ORG_CD
     						AND JIJUM_CD = SND.JIJUM_CD
     						AND MAC_NO = SND.MAC_NO), /* 코너코드							*/
     	CASE WHEN (SELECT COUNT(1) FROM T_FN_SEND WHERE ORG_CD = SND.ORG_CD
                                                  AND JIJUM_CD = SND.JIJUM_CD
                                                  AND MAC_NO = SND.MAC_NO
                                                  AND SEND_TYPE = '2'
                                                  AND SEND_DATE = TO_CHAR(SYSDATE,'YYYYMMDD')) > 0
                  AND 
                  (SELECT COUNT(1) FROM T_CM_CASH WHERE ORG_CD = SND.ORG_CD      /* 추가현송이고, 마감데이터가 없으면 마감전으로 셋팅 */
                                                  AND JIJUM_CD = SND.JIJUM_CD
                                                  AND MAC_NO = SND.MAC_NO
                                                  AND CASH_TYPE = '2') = 0 
              THEN SND.SEND_DATE
            ELSE 
              pkg_fn_common.sf_getPostActDate(SND.SEND_DATE)
            END SEND_STD_DATE,                                     	/* 현송기준일(마감전:당일, 마감후:익영업일			*/
     	CASE WHEN SND.SEND_50000_AMT IS NULL
        THEN (SELECT SEND_50000_AMT 
              FROM T_FN_SEND 
              WHERE SEND_DATE = SND.SEND_DATE
              AND ORG_CD = SND.ORG_CD
              AND JIJUM_CD = SND.JIJUM_CD
              and mac_no = SND.mac_no
              AND SEND_TIME = SND.SEND_TIME)
        ELSE
          NVL(SND.SEND_50000_AMT,0)
        END,                                     /* 장전금액(5만)					*/
     	CASE WHEN SND.SEND_10000_AMT IS NULL
        THEN (SELECT SEND_10000_AMT 
              FROM T_FN_SEND 
              WHERE SEND_DATE = SND.SEND_DATE
              AND ORG_CD = SND.ORG_CD
              AND JIJUM_CD = SND.JIJUM_CD
              and mac_no = SND.MAC_NO
              AND SEND_TIME = SND.SEND_TIME)
        ELSE
          NVL(SND.SEND_10000_AMT,0)
        END,                                     /* 장전금액(1만)					*/
     	'',
     	'',
     	'','','','','','','','','','','','','','','','','','','','','','','','','','','','','','','','','',''
FROM    T_FN_SEND_BUSAN	SND		/*	장전금액 5만,1만 필드 없음*/
WHERE SND.SEND_DATE = TO_CHAR( SYSDATE, 'YYYYMMDD' )  
AND	  NVL(SND.TRANS_YN, '0') = '0' 
AND   SND.org_cd = '031'
