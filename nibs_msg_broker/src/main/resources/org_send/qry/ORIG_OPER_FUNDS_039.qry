#USE_COL_CNT, COL_LEN, COL_TYPE 순으로 정의되어 져야 하며 SELECT 절 내부를 제외하고는 라인을 띄우면 안된다.
#SELECT 문의 COLUMN은 반드시 50개를 채워줘야 한다.(USE_COL_CNT 만큼 제외한 컬럼은 '' 등 으로 정의
USE_COL_CNT=34
COL_LEN=8,12,6,4,60,4,8,8,8,2,15,15,15,15,15,15,15,15,15,15,15,15,15,15,20,8,4,20,5,15,15,5,5,83
COL_TYPE=A,A,A,A,A,A,A,A,A,N,A,A,A,A,A,A,A,A,A,A,A,A,A,A,A,A,A,A,A,A,A,A,A,A
SELECT 	FUNS.jijum_cd,
		FUNS.mac_no,
		'',
		'',
		'',
		'',
		FUNS.demand_date,
		'',
		'',
		'',
		'',
		FUNS.post_trans_amt,
		nvl(FUNS.pre_add_trans_amt, 0) +
			nvl(d2.pre_add_trans_amt, 0),
		FUNS.pre_collect_amt,
		FUNS.differ_amt,
		FUNS.pre_respect_amt,
		FUNS.demand_amt,
		FUNS.cur_respect_amt,
		FUNS.total_stock_amt,
		'',
		'',
		'',
		'',
		'',
		'',
		'',
	    SITE.dept_cd||SITE.office_cd,
	    OFC.office_nm,
	    FUNS.pre_add_trans_cnt,
	    '',
	    '', 
	    '', 
	    '', 
	    '', '', '', '', '', '', '', '', '', '', '', '', '', '', '', '', ''
	FROM T_FN_ATMS_DEMAND_MAC FUNS,
		 T_CM_MAC MAC,
		 T_CM_SITE SITE,
		 T_CM_OFFICE OFC,
		 ( select org_cd,
                  jijum_cd,
                  mac_no,
                  sum(nvl(pre_add_trans_amt, 0)) as pre_add_trans_amt
           from      t_fn_atms_demand_mac d2
           where  org_cd = '039'
           and    jijum_cd <> '999' and mac_no <> '99'
           and      demand_date > to_char( sysdate - 30, 'YYYYMMDD' )
           and    demand_date < to_char( sysdate, 'YYYYMMDD' )
           and    org_send_yn = '0'
           group by org_cd,
                  jijum_cd,
                  mac_no					 
		 ) d2
  WHERE FUNS.org_send_yn = '0'
    AND FUNS.demand_date = to_char( sysdate, 'YYYYMMDD' )
    AND FUNS.org_cd = '039'
	AND FUNS.org_cd = d2.org_cd(+)
	AND FUNS.jijum_cd = d2.jijum_cd(+)
    AND FUNS.mac_no = d2.mac_no(+)
    AND FUNS.org_cd = MAC.org_cd(+)
    AND FUNS.jijum_cd = MAC.jijum_cd(+)
    AND FUNS.mac_no = MAC.mac_no(+)
    AND MAC.org_cd = SITE.org_cd(+)
    AND MAC.jijum_cd = SITE.jijum_cd(+)
    AND MAC.site_cd = SITE.site_cd(+)
    AND SITE.dept_cd = OFC.dept_cd(+)
    AND SITE.office_cd = OFC.office_cd(+)
ORDER BY FUNS.demand_date, FUNS.org_cd, FUNS.jijum_cd, mac_no

