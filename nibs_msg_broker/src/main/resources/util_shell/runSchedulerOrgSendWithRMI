nohup $JAVA_HOME/bin/java -server -Xms512m -Xmx1024m -cp $HOME/MsgBroker/nibs_msg_broker -Dspring.profiles.active=stage1 -Dfile.encoding=UTF-8 -DQUARTZ_NODE_NAME=OrgSend com.nicetcm.nibsplus.scheduler.main.NibsQuartzSchedulerMain 2>&1 &