PROC_NUM=`ps -ef|grep "QUARTZ_NODE_NAME=OrgSend"|grep -v "grep"|wc -l`
if [ $PROC_NUM -eq 1 ];
then
   echo "SchedulerOrgSendWithRMI is already running.."
   exit 0
fi
cd $HOME/MsgBroker
